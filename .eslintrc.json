{
  "env": {
    "commonjs": false,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true
      }
    ]
  }
}
